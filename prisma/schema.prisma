// schema.prisma  
datasource db {  
  provider = "sqlserver"  
  url      = env("DATABASE_URL")  
}  

generator client {  
  provider = "prisma-client-js"  
}  

model User {  
  id       Int      @id @default(autoincrement())  
  email    String   @unique  
  password String  
  rules    Rule[]  
}  

